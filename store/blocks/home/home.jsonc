  "newsletter-form": {
    "children": [
      "responsive-layout.desktop",
      "responsive-layout.phone"
    ]
  },

  "responsive-layout.desktop": {
    "children": ["flex-layout.row#newsletter"],
    "props": {
      "blockClass": "newsletter"
    }
  },

   "responsive-layout.phone": {
    "children": [
      "rich-text#newsletter-title",
      "flex-layout.row#newsletter2"
    ],
    "props": {
      "blockClass": "newsletter-mobile"
    }
  },

  "flex-layout.row#newsletter": {
    "children": [
      "rich-text#newsletter-title",
      "newsletter-input-email",
      "newsletter-submit"
    ]
  },

  "flex-layout.row#newsletter2": {
    "children": [
      "newsletter-input-email",
      "newsletter-submit"
    ],
    "props": {
      "blockClass": "newsletter-mobile"
    }
  },

  "rich-text#newsletter-title": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "**Suscribite al Newsletter**\nRecibí ofertas y novedades en tu mail",
      "textColor": "c-on-emphasis",
      "font": "t-body",
      "blockClass": "newsletter-title"
    }
  },

  "newsletter-input-email": {
    "props": {
      "placeholderText": "Escribí tu email",
      "errorMessage": "*Por favor, ingrese un email válido",
      "blockClass": "newsletter-input"
    }
  },

  "newsletter-submit": {
    "props": {
      "submitButtonLabel": "▶",
      "blockClass": "newsletter-submit"
    }
  }

