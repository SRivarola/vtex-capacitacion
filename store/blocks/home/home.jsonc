      "height": 630,
      "preload": true,
      "images": [
        {
          "image": "https://firebasestorage.googleapis.com/v0/b/etana2.appspot.com/o/vtex%2Fport1tiny.jpg?alt=media&token=a70b1585-862c-464d-a8ab-46b1ab2e49b0",
