    }
  },

  "slider-layout#banner-images": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "autoplay": {
        "timeout": 7000,
        "stopOnHover": true
      },
      "showNavigationArrows": "desktopOnly",
      "blockClass": "carousel"
    }
  },

  "rich-text#shelf-title": {
    "props": {
      "text": "## **Colección 22**",
      "blockClass": "shelfTitle"
    }
  },

  "flex-layout.row#shelf": {
    "children": ["list-context.product-list#demo1"],
    "props": {
      "blockClass": "shelf-slider"
    }
  },
  
  "list-context.product-list#demo1": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#demo-products"],
    "props": {
      "orderBy": "OrderByReleaseDateDESC",
      "category": "14/15"
    }
  },
  
  "slider-layout#demo-products": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 1
      },
      "infinite": true,
      "fullWidth": false,
      "blockClass": "shelf"
    }
  },

  "flex-layout.row#cards1": {
    "children": [
      "info-card#home1",
      "info-card#home2"
    ]
  },
  
  "info-card#home1": {
    "props": {
      "id": "info-card-home",
      "isFullModeStyle": true,
      "textPosition": "center",
      "imageUrl": "https://firebasestorage.googleapis.com/v0/b/etana2.appspot.com/o/vtex%2F3.jpg?alt=media&token=8ba1adbb-8421-4f35-ad33-449dd3b877cb",
      "headline": "BOTINETAS",
      "callToActionText": "VER",
      "callToActionUrl": "/collections/botinetas",
